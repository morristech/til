    "build": "gatsby build",
    "tsc:check": "tsc",
    "tsc:watch": "tsc --watch",
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "prettier:fix": "prettier --write 'src/**/*.{ts,tsx,js,jsx}' 'posts/**/*.md'",
    "prettier:check": "prettier --list-different 'src/**/*.{ts,tsx,js,jsx}' 'posts/**/*.md'",
    "prettier:watch": "onchange 'src/**/*.{ts,tsx,js,jsx}' 'posts/**/*.md' -- prettier --list-different '{{changed}}'",
    "ci": "run-s tsc:check prettier:check",
    "ci:watch": "run-p -sl tsc:watch prettier:watch",
    "fix": "run-s css_modules:update prettier:fix",
    "test": "echo \"Error: no test specified\" && exit 1",
    "css_modules:update": "tcm src/; prettier --write 'src/**/*.module.*.d.ts'"
  },
